# System
alias clear="tput reset"
alias vim="nvim"
alias configvim="nvim ~/.config/nvim/"
alias configalias="nvim ~/.aliasrc"
alias configbash="nvim ~/.bashrc"

# Projetos
alias cppcompile="g++ -Wall -Wconversion --verbose $1"

# Directory
alias projeto="cd ~/Documentos/Software/Projetos/"
alias scheduling="cd ~/Documentos/Software/Scheduling/"

